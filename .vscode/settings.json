{
    "python.analysis.extraPaths": [
        "./tools/splat",
        "./tools/iso",
    ],
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.exclude": {
        "expected/": true
    },
    "files.associations": {
        "*.h": "c",
    },
    "[python]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "ms-python.black-formatter",
    },
    "[rust]": {
        "editor.formatOnSave": true,
    },
    "search.useIgnoreFiles": false
}
